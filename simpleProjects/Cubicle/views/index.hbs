<h1>Browser</h1>
<form action="search">
  <input type="text" class="search" name="searchName" placeholder="Search...">
  <input type="number" name="from" class="difficulty" placeholder="Difficulty from...">
  <span>-</span>
  <input type="number" name="to" class="difficulty" placeholder="Difficulty to...">
  <input type="submit" value="search">
</form>

{{#each cubs}}

<div class="cube">
  <img class="cube" src="{{imageUrl}}">
  <p class="name">{{name}}</p>
  <p><span>Difficulty level:</span>{{difficulty}}</p>
  <a class="swd-button" href="{{_id}}">Details</a>
</div>
{{/each}}